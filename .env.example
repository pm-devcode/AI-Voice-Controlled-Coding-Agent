# === Required ===
GEMINI_API_KEY=your_gemini_api_key_here

# === Optional: TTS ===
ELEVENLABS_API_KEY=your_elevenlabs_key_here
TTS_PROVIDER=kokoro  # Options: kokoro, elevenlabs, gtts, disabled

# === Server Config ===
BACKEND_HOST=127.0.0.1
BACKEND_PORT=8765
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR

# === Model Config ===
WHISPER_MODEL=large-v3  # Options: tiny, base, small, medium, large-v3
WHISPER_DEVICE=cuda     # Options: cuda, cpu
WHISPER_COMPUTE_TYPE=float16  # Options: float16, int8, float32

# === Agent Config ===
GEMINI_MODEL_FAST=gemini-2.0-flash
GEMINI_MODEL_THINKING=gemini-2.0-flash-thinking-exp
MAX_CONTEXT_TOKENS=128000
